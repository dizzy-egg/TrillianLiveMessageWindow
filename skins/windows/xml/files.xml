<!-- Fonts -->
	<font name="segoeui" file="segoeui.ttf"/>

	<theme name="default" description="Blue">
		<filter name="hueshift">
			<hue num="0"/>
			<saturation num="0"/>
			<brightness num="0"/>
		</filter>
	</theme>

	<theme name="follow" description="Follow Windows">
		<filter name="hueshift">
			<hue num="%system.windows.hue%"/>
			<saturation num="%system.windows.saturation%"/>
			<brightness num="0"/>
		</filter>
	</theme>

